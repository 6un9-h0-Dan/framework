  "require": {
    "php": ">=7.0",
    "spiral/common": "^0.9",
    "spiral/database": "^0.9",
    "spiral/orm": "^0.9",
    "spiral/storage": "^0.9",
    "spiral/stempler": "^0.9",
    "twig/twig": "^2.1",
    "zendframework/zend-diactoros": "^1.1",
    "symfony/console": "^3.2",
    "symfony/process": "^3.2",
    "symfony/translation": "^3.2",
    "cocur/slugify": "^2.3",
    "defuse/php-encryption": "^2.0",
    "monolog/monolog": "^1.22",
    "vlucas/phpdotenv": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "^0.9.4",
    "spiral/odm": "^0.9"
  },
  "suggest": {
    "spiral/odm": "Compositable ODM"
  },
  "autoload": {
    "files": [
      "source/functions.php"
