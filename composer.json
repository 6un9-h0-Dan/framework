{
    "name": "spiral/framework",
    "type": "library",
    "description": "Spiral, High-Performance PHP/Go Framework",
    "homepage": "https://spiral.dev",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/framework"
    },
    "authors": [
        {
            "name": "Wolfy-J",
            "email": "wolfy.jd@gmail.com"
        }
    ],
    "bin": [
        "bin/spiral"
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-mbstring": "*",
        "codedungeon/php-cli-colors": "~1.0",
        "defuse/php-encryption": "^2.2",
        "psr/container": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.0",
        "spiral/composer-publish-plugin": "^1.0",
        "spiral/tokenizer": "^2.0",
        "symfony/console": "^2.5|^3.0|^4.0|^5.0",
        "symfony/polyfill-php80": "^1.18"
    },
    "autoload": {
        "files": [
            "src/Boot/src/helpers.php",
            "src/Dumper/src/helpers.php",
            "src/Framework/helpers.php"
        ],
        "psr-4": {
            "Spiral\\": "src/Framework",
            "Spiral\\Auth\\": "src/Auth/src",
            "Spiral\\Boot\\": "src/Boot/src",
            "Spiral\\Config\\": "src/Config/src",
            "Spiral\\Console\\": "src/Console/src",
            "Spiral\\Cookies\\": "src/Cookies/src",
            "Spiral\\Core\\": "src/Core/src",
            "Spiral\\Debug\\": [
                "src/Debug/src",
                "src/Dumper/src"
            ],
            "Spiral\\Encrypter\\": "src/Encrypter/src",
            "Spiral\\Exceptions\\": "src/Exceptions/src",
            "Spiral\\Files\\": "src/Files/src",
            "Spiral\\Http\\": "src/Http/src",
            "Spiral\\Logger\\": "src/Logger/src",
            "Spiral\\Models\\": "src/Models/src",
            "Spiral\\Streams\\": "src/Streams/src"
        }
    },
    "require-dev": {
        "cycle/annotated": "^2.0",
        "cycle/migrations": "^1.0",
        "cycle/orm": "^1.0",
        "cycle/proxy-factory": "^1.0",
        "laminas/laminas-diactoros": "^2.3",
        "mockery/mockery": "^1.1",
        "phpunit/phpunit": "^8.5|^9.0",
        "roave/security-advisories": "dev-master",
        "spiral/auth-http": "^1.1",
        "spiral/broadcast": "^2.0",
        "spiral/broadcast-ws": "^1.0",
        "spiral/code-style": "^1.0",
        "spiral/csrf": "^1.1",
        "spiral/database": "^2.4",
        "spiral/filters": "^1.0",
        "spiral/jobs": "^2.0",
        "spiral/migrations": "^2.0",
        "spiral/php-grpc": "^1.0",
        "spiral/roadrunner": "^1.2",
        "spiral/router": "^1.0",
        "spiral/security": "^2.0",
        "spiral/session": "^1.2.2",
        "spiral/snapshots": "^1.0",
        "spiral/translator": "^1.2",
        "spiral/validation": "^1.4.4",
        "spiral/views": "^1.0",
        "symplify/monorepo-builder": "^7.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\App\\": "tests/app/src",
            "Spiral\\Tests\\Auth\\": "src/Auth/tests",
            "Spiral\\Tests\\Boot\\": "src/Boot/tests",
            "Spiral\\Tests\\Config\\": "src/Config/tests",
            "Spiral\\Tests\\Console\\": "src/Console/tests",
            "Spiral\\Tests\\Cookies\\": "src/Cookies/tests",
            "Spiral\\Tests\\Core\\": "src/Core/tests",
            "Spiral\\Tests\\Debug\\": [
                "src/Debug/tests",
                "src/Dumper/tests"
            ],
            "Spiral\\Tests\\Encrypter\\": "src/Encrypter/tests",
            "Spiral\\Tests\\Exceptions\\": "src/Exceptions/tests",
            "Spiral\\Tests\\Files\\": "src/Files/tests",
            "Spiral\\Tests\\Framework\\": "tests/Framework",
            "Spiral\\Tests\\Http\\": "src/Http/tests",
            "Spiral\\Tests\\Logger\\": "src/Logger/tests",
            "Spiral\\Tests\\Models\\": "src/Models/tests",
            "Spiral\\Tests\\Streams\\": "src/Streams/tests"
        }
    },
    "replace": {
        "spiral/auth": "*",
        "spiral/boot": "*",
        "spiral/config": "*",
        "spiral/console": "*",
        "spiral/cookies": "self.version",
        "spiral/core": "*",
        "spiral/debug": "*",
        "spiral/dumper": "*",
        "spiral/encrypter": "*",
        "spiral/exceptions": "*",
        "spiral/files": "*",
        "spiral/http": "*",
        "spiral/logger": "*",
        "spiral/models": "*",
        "spiral/streams": "*"
    },
    "scripts": {
        "merge": [
            "monorepo-builder merge",
            "@php bin/sync"
        ],
        "split": "monorepo-builder split"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.6.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}