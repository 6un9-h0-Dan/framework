{
    "name": "spiral/jobs",
    "type": "library",
    "description": "Spiral: Jobs Jobs and Queue",
    "license": "MIT",
    "authors": [
        {
            "name": "Wolfy-J",
            "email": "wolfy.jd@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "spiral/roadrunner": "^2.4",
        "spiral/roadrunner-jobs": "^2.0"
    },
    "autoload": {
        "files": [
            "src/polyfill.php"
        ],
        "psr-4": {
            "Spiral\\Jobs\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0",
        "spiral/core": "^2.9",
        "doctrine/inflector": "^1.4|^2.0",
        "spiral/code-style": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Jobs\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.9.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
